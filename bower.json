{
  "name": "rules",
  "main": [
    "/dist/rules.min.js",
    "/dist/rules.min.css"
  ],
  "version": "0.1.0",
  "description": "VDMS angular app",
  "authors": [
    "Verizon Digital Media Services - UI"
  ],
  "ignore": [
    "**/.*",
    "package.json",
    "klei.json",
    "gulpfile.js",
    "node_modules",
    "bower_components",
    "src"
  ],
  "dependencies": {
    "angular": "~1.3.14",
    "angular-ui-router": "*",
    "angular-resource": "*",
    "angular-sanitize": "*",
    "lodash": "~3.1.0",
    "x2js": "~1.1.7",
    "jsdiff": "~1.0.8",
    "ace-builds": "~1.1.7",
    "angular-ui-ace": "~0.1.1",
    "angular-ui-utils": "*",
    "ng-sortable": "1.2.0"
  },
  "devDependencies": {
    "angular-mocks": "*"
  },
  "overrides": {
    "ace-builds": {
      "main": [
        "src-min/ace.js",
        "src-min/mode-xml.js"
      ]
    },
    "jsdiff": {
      "main": "diff.js"
    },
    "x2js": {
      "main": "xml2json.min.js"
    },
    "bootstrap": {
      "main": [
        "js/tooltip.js",
        "js/popover.js"
      ]
    }
  },
  "resolutions": {
    "angular": ">=1.2 <=1.3.x"
  }
}
